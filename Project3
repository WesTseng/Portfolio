pip install requests
pip install lxml
pip install bs4
import requests
import lxml
import bs4

result = requests.get("https://en.wikipedia.org/wiki/2018_FIFA_World_Cup")
result.text

#Grabbing elements separately from the title section of the HTML
soup = bs4.BeautifulSoup4(result.text,"lxml")
soup
soup.select('title')[0].getText()

#Grabbing class elements
soup.select('.toctext")
